<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns:html="http://www.w3.org/1999/xhtml" >
<Page id="resources" showHeader="false">
				<content>
					<VBox width="100%" height="100%" justifyContent="Center" alignItems="Center">
						<VBox>
								<core:Fragment fragmentName="POCAIRE.fragment.GestaoRecursosT1" type="XML"></core:Fragment>
								<core:Fragment fragmentName="POCAIRE.fragment.GestaoRecursosT2" type="XML"></core:Fragment>
							
							<HBox width="100%" justifyContent="SpaceBetween">
								<ToggleButton text="{i18n>gantt}" enabled="true" pressed="false" press="onPress" class="ganttbutton"/>
							<HBox >
								<Button icon="sap-icon://navigation-left-arrow"/>
								<Button icon="sap-icon://navigation-right-arrow"/> 
							</HBox>
							
							</HBox>
						</VBox>
						
					</VBox>
				</content>
			</Page>





	</core:FragmentDefinition>